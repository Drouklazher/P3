<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.HistoryActivity"
    android:id="@+id/main">

    <android.support.v7.widget.AppCompatButton
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/fullHistoryButton"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintBottom_toTopOf="@+id/historyDayTwo"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDayOne"/>


    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/historyDayThree"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDayTwo"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/historyDayFour"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDayThree"
        app:layout_constraintVertical_weight="1"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/historyDayFive"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDayFour"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/historyDaySix"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDayFive"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/historyDaySeven"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDaySix"/>

    <android.support.v7.widget.AppCompatTextView
        android:text="blub"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent=".125"
        app:layout_constraintBottom_toTopOf="@+id/fullHistoryButton"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/historyDaySeven"/>

</android.support.constraint.ConstraintLayout>